{
  "global_parameters": {
    "debug": false,
    "display": false
  },
  "tensorflow_configs": {
    "device": "/gpu:0",
    "per_process_gpu_memory_fraction": 0.9,
    "allow_soft_placement": true,
    "log_device_placement": false
  },
  "network": {
    "model": "model",
    "parameters": {
      "dropout": 0.8
    }
  },
  "trainer": {
    "parameters": {
      "batch_size": 10,
      "num_epochs": 2,
      "shuffle_buffer_size": 10000,
      "learning_rate": 0.001
    }
  },
  "dataset": {
    "original": {
      "path": "../datasets/kaggle/imaterialist-challenge-fashion-2018",
      "train_folder": "train",
      "test_folder": "test",
      "validation_folder": "validation"
    },
    "parameters": {
      "load_mode": "tf",
      "load_type": "h5",
      "width": 300,
      "height": 300,
      "channels": 3,
      "n_classes": 228
    },
    "lists": {
      "folder": "lists",
      "train": "train.list",
      "validation": "validation.list",
      "test": "test.list"
    },
    "h5": {
      "train_pattern": "train",
      "test_pattern": "test",
      "validation_pattern": "validation"
    },
    "folders": {
      "h5_folder": "h5"
    }
  },
  "metadata": {
    "path": "../metadata/ck-experiments",
    "models": {
      "folder": "models/model"
    },
    "logs": {
      "train_folder": "train",
      "test_folder": "test"
    }
  }
}