{
  "global_parameters": {
    "debug": false,
    "display": false
  },
  "tensorflow_configs": {
    "device": "/gpu:0",
    "per_process_gpu_memory_fraction": 0.9,
    "allow_soft_placement": true,
    "log_device_placement": false
  },
  "dataset": {
    "original": {
      "path": "../datasets/kaggle/imaterialist-challenge-fashion-2018",
      "train_folder": "train",
      "test_folder": "test",
      "validation_folder": "validation"
    },
    "parameters": {
      "width": 80,
      "height": 80,
      "channels": 3,
      "n_classes": 7
    },
    "lists": {
      "folder": "lists",
      "train": "train.list",
      "validation": "validation.list",
      "test": "test.list"
    }
  },
  "metadata": {
    "path": "../metadata/ck-experiments",
    "models": {
      "folder": "models/model"
    },
    "logs": {
      "train_folder": "train",
      "test_folder": "test"
    }
  }
}